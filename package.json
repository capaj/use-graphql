    "coverage": "jest --coverage",
    "watch": "npm run build -- --watch",
    "w": "jest --watch"
  },
  "peerDependencies": {
    "graphql": "14.1.1",
    "graphql-request": "1.8.2",
    "react": "16.8.3"
  },
  "devDependencies": {
    "@types/graphql": "^14.0.5",
    "@types/jest": "^24.0.0",
    "@types/node": "^10.12.24",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "coveralls": "^3.0.2",
    "graphql": "14.1.1",
    "graphql-request": "1.8.2",
    "graphql-tag": "^2.10.1",
    "jest": "^24.1.0",
    "prettier": "^1.16.4",
    "react": "16.8.1",
    "react-dom": "^16.8.3",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {}
